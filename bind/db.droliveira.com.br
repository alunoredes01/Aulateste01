;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	droliveira.com.br. root.droliveira.com.br. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	servidor1.droliveira.com.br.
@	IN	A	192.168.27.10
;@	IN	AAAA	::1
servidor1	IN	A	192.168.27.10
dns1	IN	CNAME	servidor1.droliveira.com.br.
dns2	IN	A	192.168.27.20
cliente IN	A	192.168.27.30
www	IN	A	192.168.27.10
dns	IN	A	192.168.27.10
